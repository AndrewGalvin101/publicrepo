<br />

<div class="container body-content">

    <div class="container" style="border:solid">
        <br />
        <form id="inventorySearch" class="col">
            <h5>Quick Search</h5>
            <div class="form-inline">
                <input type="text" class="form-control col-md-4" id="quickSearch" placeholder="Enter make, model or year" />
                <div class="form-inline col-md-8">
                    <label class="col-md-1 justify-content-end"><strong>Price</strong></label>
                    <input type="number" class="form-control col-md-2" id="minPrice" placeholder="No Min" />
                    <input type="number" class="form-control col-md-2" id="maxPrice" placeholder="No Max" />
                    <label class="col-sm-2 justify-content-end"><strong>Year</strong></label>
                    <input type="number" class="form-control col-md-2" id="minYear" placeholder="No Min" />
                    <input type="number" class="form-control col-md-2" id="maxYear" placeholder="No Max" />
                </div>
            </div><br />
            <div class="form-inline">
                <div class="col-sm-4">
                </div>
                <button type="submit" class="btn btn-outline-dark col-md-4">Search</button>
            </div>
        </form>
        <br />
    </div>
    <br />
    <div id="searchResults">

    </div>

</div>
